vendorize(property, value)
	-webkit-{property} value
	-moz-{property} value
	{property} value

white = #fff
black = #000
greentext = #2e4520
greentext2 = #678450
greenbg = #62804a

@font-face
	font-family 'Constantia'
	src url("../fonts/Constantia.eot")
	src url("../fonts/Constantia.eot?#iefix")format("embedded-opentype"),
		url("../fonts/Constantia.woff2") format("woff2"),
		url("../fonts/Constantia.woff") format("woff"),
		url("../fonts/Constantia.ttf") format("truetype"),
		url("../fonts/Constantia.svg#Constantia") format('svg')
	font-style normal
	font-weight normal

@font-face
	font-family 'ConstantiaBold'
	src url("../fonts/ConstantiaBold.eot")
	src url("../fonts/ConstantiaBold.eot?#iefix")format("embedded-opentype"),
		url("../fonts/ConstantiaBold.woff2") format("woff2"),
		url("../fonts/ConstantiaBold.woff") format("woff"),
		url("../fonts/ConstantiaBold.ttf") format("truetype"),
		url("../fonts/ConstantiaBold.svg#ConstantiaBold") format('svg')
	font-style normal
	font-weight normal


/*==============================
	Common styles
==============================*/
html
body
	height 100%
body
	font-family 'Open Sans', sans-serif
	background-color white
	-webkit-font-smoothing antialiased
	&.promo-page
		margin 0
		padding 0
		background-color black
	&.cabinet-page
		background-color #f6f6f6
		
button
	padding 0
	border none
	background-color transparent
	&:focus
		outline none
a
	vendorize(transition,.4s ease)
	&:hover
	&:active
	&:focus
		outline none
		text-decoration none

input[type="text"]
input[type="password"]
select
textarea
	&:focus
		outline none

::-moz-selection
	background white
	color black
	text-shadow none
::-webkit-selection
	background white
	color black
	text-shadow none
::selection
	background white
	color black
	text-shadow none

::-webkit-input-placeholder
	color white
	opacity 1
::-moz-placeholder
	color white
	opacity 1
:-moz-placeholder
	color white
	opacity 1
:-ms-input-placeholder
	color white
	opacity 1

[type="checkbox"]
	&:not(:checked)
	&:checked
		position absolute
		left -9999px
[type="checkbox"]:not(:checked) + label
[type="checkbox"]:checked + label
	position relative
	padding-left 40px
	cursor pointer
	font-size 14px
	color greentext
	margin-bottom 0
	font-weight 400
	a
		color greentext2
		text-decoration underline
		&:hover
			text-decoration none
[type="checkbox"]:not(:checked) + label:before
[type="checkbox"]:checked + label:before
	content ''
	position absolute
	left 0
	top 2px
	width 20px
	height 20px
	background #ddd
	border-radius 4px
[type="checkbox"]:not(:checked) + label:after
[type="checkbox"]:checked + label:after
	font-family 'Glyphicons Halflings'
	content '\e013'
	position absolute
	left 0
	top 2px
	width 20px
	height 20px
	font-size 14px
	line-height 20px
	color white
	text-align center
	vendorize(transition,.2s)
	background-color greenbg
	border-radius 4px
[type="checkbox"]:not(:checked) + label:after
	opacity 0
	transform scale(0)
[type="checkbox"]:checked + label:after
	opacity 1
	transform scale(1)

